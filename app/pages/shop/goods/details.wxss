.screen-swiper {
    min-height:555rpx;
}
/*-------左上角按钮-------*/
.canui-head-box{
    position:fixed;
    top:26px;
    margin-left:10px;
    background:rgba(0,0,0,0.2);
    color:#fff;
    border-radius:20px;
    padding:4px 4px;
    border:1px solid rgba(255,255,255,0.3);
}
.canui-head-box navigator{
    padding:8px;
    font-size:20px;
    display:initial;
}
.canui-head-box navigator + navigator{
    border-left:1px solid rgba(255,255,255,0.3);
    margin:0 3px;
    padding:0 5px;
    padding-left:15px;
}

.canui-goods-box{
    padding:10rpx 30rpx;
}

.canui-xzwz{
    -webkit-line-clamp:2;
}
.cauni-details-box{
    word-wrap:break-word;
}
.cu-bar.tabbar.shop .action {
    width:100rpx;
}
.cu-bar.tabbar .action {
    font-size:20rpx;
}
.cu-bar.tabbar .action [class*="icon"] {
    width:38rpx;
}
.canui-flex-car{
    padding-top: 10px;
}
.canui-flex-car .cu-card>.cu-item {
    margin: 0px;
    position: relative;
}
.canui-flex-car .cu-list.menu-avatar>.cu-item {
    padding-left:15rpx;
    height:100rpx;
}
.canui-flex-car .cu-card .cu-avatar{
    position:absolute;
    right:10px;
    bottom:72rpx;
}
.canui-flex-car .shadow-warp {
    box-shadow:0 0 30rpx rgba(0, 0, 0, 0.1);
}
.canui-flex-car .cu-list.menu>.cu-item .content {
    font-size:26rpx;
}
.canui-flex-car .text-sm {
    font-size:20rpx;
}
.canui-flex-car .text-grey {
    color: #000;
}
.canui-flex-car image{
    height: 90px !important;
}
.canui-gps .justify-between{
    padding: 0 10px;
}

.content .text-sm{
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:1;
}

.canui-fixed{
    position: fixed;
    bottom: 0px;
    width: 100%;
    z-index:9999
}